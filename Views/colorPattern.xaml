<Window x:Class="MVVM_Football_Informant.Views.colorPattern"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVM_Football_Informant.Views"
        mc:Ignorable="d"
        Title="colorPattern" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Button Background="#04196D"
                Grid.Column="0"/>

        <Button Background="#00008C"
                Grid.Column="1"/>

        <Button Background="#299617"
                Grid.Column="2"/>

        <Button Background="#087830"
                Grid.Column="3"/>

        <Button Background="#014530"
                Grid.Column="4"/>

        <Button Background="#100C08"
                Grid.Column="5"/>
    </Grid>
</Window>
